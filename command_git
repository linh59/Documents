 onRowClick: function (e) {
            if (e.rowType == 'group') {
                if (e.isExpanded)
                    e.component.collapseRow(e.key);
                else
                    e.component.expandRow(e.key);
            }
        },
		
 .OnRowClick("collapseRow")
<script type="text/javascript">
    function collapseRow(e) {
         if (e.rowType == 'group') {
                if (e.isExpanded)
                    e.component.collapseRow(e.key);
                else
                    e.component.expandRow(e.key);
            }
        }
</script>

